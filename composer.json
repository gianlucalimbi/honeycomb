{
    "name": "gianlucalimbi/honeycomb",
    "description": "Sweet JSON API for Laravel",
    "version": "1.0.0",
    "type": "library",
    "keywords": [
        "api",
        "json",
        "rest",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Gianluca Limbi",
            "email": "info@gianlucalimbi.com"
        }
    ],
    "require": {
        "laravel/framework": "~5.1"
    },
    "require-dev": {
        "laravel/framework": "5.1.*"
    },
    "autoload": {
        "psr-4": {
            "Honeycomb\\": "src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Honeycomb\\HoneycombServiceProvider"
            ]
        }
    }
}
